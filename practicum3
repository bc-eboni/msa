   "source": [
    "# plot three GAM-smoothed lines against raw data\n",
    "options(repr.plot.width = 25, repr.plot.height = 6)\n",
    "\n",
    "par(mfrow=c(1,3))\n",
    "\n",
    "ts.plot(country.red.ts, xlab=\"Decade\", ylab=\"Value\", main=\"France Dominant Red by Decade\")\n",
    "lines(country.red.fit.gam, lwd=2, col=\"red\")\n",
    "\n",
    "ts.plot(country.green.ts, xlab=\"Decade\", ylab=\"Value\", main=\"France Dominant Green by Decade\")\n",
    "lines(country.green.fit.gam, lwd=2, col=\"green\")\n",
    "\n",
    "ts.plot(country.blue.ts, xlab=\"Decade\", ylab=\"Value\", main=\"France Dominant Blue by Decade\")\n",
    "lines(country.blue.fit.gam, lwd=2, col=\"blue\")"
   ]
  },
