   "source": [
    "query = \"\n",
    "SELECT year_start AS 'decade',\n",
    "       color1_r AS 'red',\n",
    "       color1_g AS 'green',\n",
    "       color1_b AS 'blue'\n",
    "FROM color_stats\n",
    "WHERE country='France'\n",
    "  AND painting_count >=3\n",
    "ORDER BY decade ASC\"\n",
    "\n",
    "\n",
    "country = dbGetQuery(conn, query)\n",
    "tmp <- data.frame(decade=as.integer(seq(min(country$decade), max(country$decade), 10)))\n",
    "country.df <- merge(country, tmp, by='decade', all=T)\n",
    "country.df[is.na(country.df)] <- NA\n",
    "\n",
    "options(repr.plot.width = 12, repr.plot.height = 6)\n",
    "ts.plot(country.df$red,\n",
    "        col='red',\n",
    "        ylab=\"Dominant R Value\",\n",
    "        main=\"France Dominant R: Raw Data\")"
   ]
  },
