    "country.df <- country.df %>%\n",
    "        mutate(red = na.approx(red))\n",
    "country.df <- country.df %>%\n",
    "        mutate(green = na.approx(green))\n",
    "country.df <- country.df %>%\n",
    "        mutate(blue = na.approx(blue))\n",
